  "bugs": {
    "url": "https://github.com/KevDev90/Food-Finder-Server/issues"
  },
  "homepage": "https://github.com/KevDev90/Food-Finder-Server#readme",
  "dependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "knex": "^0.95.4",
    "mocha": "^8.3.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "postgrator": "^4.1.1",
    "postgrator-cli": "^4.0.0",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "sinon": "^10.0.0"
  }
}
